        xmlns:local="clr-namespace:Micro_Project"
        mc:Ignorable="d"
        Title="EditReg" Height="450" Width="800">
    <Grid Background="#FF262626">
        <DatePicker x:Name="dateTimePickerStart" HorizontalAlignment="Left" Margin="10,21,0,0" VerticalAlignment="Top" Width="162"/>
        <DatePicker x:Name="dateTimePickerEnd" HorizontalAlignment="Left" Margin="191,21,0,0" VerticalAlignment="Top" Width="162"/>
        <Button x:Name="BAddNomer" Content="Добавить" HorizontalAlignment="Left" Margin="525,21,0,0" VerticalAlignment="Top" Width="229" Height="24" Background="#FF262626" BorderBrush="White" Foreground="White" Click="BAddNomer_Click"/>
        <DataGrid x:Name="DataGridViewClient" HorizontalAlignment="Left" Height="153" Margin="0,85,0,0" VerticalAlignment="Top" Width="792"/>
        <DataGrid x:Name="DataGridViewNomer" HorizontalAlignment="Left" Height="158" Margin="0,261,0,0" VerticalAlignment="Top" Width="792"/>
        <ComboBox x:Name="CBDesclive" HorizontalAlignment="Left" Margin="370,23,0,0" VerticalAlignment="Top" Width="139"/>

    </Grid>
</Window>
